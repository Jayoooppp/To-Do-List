<%- include("header")  -%> 
    <div class="box" id="heading">
        <h1><%= title %></h1>
    </div>

    <div class="box">
        <% for(var i =0; i < newitems.length; i++) {%>
            <div class="item">
                <input type="checkbox">
                <p><%= newitems[i] %></p>
            </div>
        <%}%>

        <form action="/" method="post" class="item">
            <input type="text" name="item" placeholder="Add Item" autocomplete="off">
            <button type="submit" name="button" value="<%= title %>">+</button>
        </form>
        
    </div>

    <%- include("footer")  -%> 


